{
    "name": "My custom new tab page",
    "description": "Overrides the new tab page",
    "version": "0.1",
    "incognito": "split",
    "chrome_url_overrides": {
      "newtab": "index.html"
    },
    "content_security_policy": "script-src 'self' https://example.com; object-src 'self'",
    "manifest_version": 2
  }